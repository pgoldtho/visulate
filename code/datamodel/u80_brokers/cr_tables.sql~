create table ldr_license_edu
( lRank                varchar2(2)
, Title                varchar2(20)
, Lic                  varchar2(20)
, Name                 varchar2(64)
, Addr1                varchar2(64)
, Addr2                varchar2(64)
, City_state_zip       varchar2(64)
, Lic_Expire_Date      date
, Course_Number        varchar2(16)
, Course_Name          varchar2(64)
, Course_Credit_Hours  number
, Course_End_Date      date );