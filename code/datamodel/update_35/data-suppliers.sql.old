INSERT INTO RNT_SUPPLIERS_ALL (
   SUPPLIER_ID, NAME, PHONE1, 
   PHONE2, ADDRESS1, ADDRESS2, 
   CITY, STATE, ZIPCODE, 
   EMAIL_ADDRESS, COMMENTS, SUPPLIER_TYPE_ID) 
SELECT 
   SUPPLIER_ID, NAME, PHONE1, 
   PHONE2, ADDRESS1, ADDRESS2, 
   CITY, STATE, ZIPCODE, 
   EMAIL_ADDRESS, COMMENTS, 1 as SUPPLIER_TYPE_ID
FROM RNT_SUPPLIERS
/


INSERT INTO RNT_BU_SUPPLIERS (
   BU_SUPPLIER_ID, BUSINESS_ID, SUPPLIER_ID, 
   TAX_IDENTIFIER, NOTES) 
SELECT
   RNT_BU_SUPPLIERS_SEQ.NEXTVAL,  
   BUSINESS_ID, SUPPLIER_ID, SSN, NULL as NOTES 
FROM RNT_SUPPLIERS
/

 
